{
  "name": "UNHCR",
  "version": "0.0.0",
  "description": "Map of services available to refugees in Jordan",
  "main": "index.js",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "globals": {
      "angular": true,
      "jQuery": true,
      "L": true,
      "$": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none"
        }
      ]
    }
  },
  "scripts": {
    "watch": "gulp watch",
    "build": "gulp dist",
    "install": "napa unhcr/Humanitarian-Font"
  },
  "directories": {
    "doc": "doc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gilf/ngPrint.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.3.1",
    "browserify": "^14.4.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watchify": "^0.7.0",
    "lazy": "1.0.11",
    "napa": "^3.0.0",
    "rev-del": "^1.0.5",
    "run-sequence": "^2.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^3.2.0"
  },
  "dependencies": {
    "angular-translate-loader-static-files": "^2.12.1",
    "crossfilter": "^1.3.7",
    "drmonty-leaflet-awesome-markers": "^2.0.2",
    "fuse.js": "^3.0.5",
    "geojson-utils": "^1.1.0",
    "glob": "^5.0.5",
    "leaflet": "^0.7.2",
    "leaflet.markercluster": "Leaflet/Leaflet.markercluster#232e93cc",
    "mapbox.js": "^1.6.3",
    "measure-time": "^3.1.0",
    "ngPrint": "git+https://github.com/gilf/ngPrint.git",
    "request-promise": "^4.2.1",
    "split": "^0.3.0",
    "tree-model": "^1.0.6",
    "underscore": "*"
  }
