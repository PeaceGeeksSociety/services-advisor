{
  "name": "UNHCR",
  "version": "0.0.0",
  "description": "Map of services available to refugees in Jordan",
  "main": "index.js",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "globals": {
      "angular": true,
      "jQuery": true,
      "L": true,
      "$": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none"
        }
      ]
    }
  },
  "scripts": {
    "start": "webpack-dev-server --compress --open",
    "serve": "webpack-dev-server --compress",
    "watch": "webpack -d --watch --progress --colors --config ./webpack.config.js",
    "build-dev": "webpack -d --progress --colors --config ./webpack.config.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config ./webpack.config.js",
    "install": "napa unhcr/Humanitarian-Font"
  },
  "directories": {
    "doc": "doc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gilf/ngPrint.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.3.1",
    "browserify": "^14.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watchify": "^0.7.0",
    "html-webpack-plugin": "^2.30.1",
    "lazy": "1.0.11",
    "napa": "^3.0.0",
    "rev-del": "^1.0.5",
    "run-sequence": "^2.1.0",
    "sass-loader": "^6.0.6",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^3.2.0",
    "webpack": "^3.5.5",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "angular-translate-loader-static-files": "^2.12.1",
    "async": "^2.5.0",
    "crossfilter": "^1.3.7",
    "drmonty-leaflet-awesome-markers": "^2.0.2",
    "fuse.js": "^3.0.5",
    "geojson-utils": "^1.1.0",
    "glob": "^5.0.5",
    "leaflet": "^1.2.0",
    "leaflet.markercluster": "^1.1.0",
    "mapbox.js": "^3.1.1",
    "measure-time": "^3.1.0",
    "ngPrint": "git+https://github.com/gilf/ngPrint.git",
    "request-promise": "^4.2.1",
    "split": "^0.3.0",
    "tree-model": "^1.0.6",
    "underscore": "*",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
