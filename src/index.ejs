<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Services Advisor</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="shortcut icon" type="image/ico" href="favicon.ico" />
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
  <link rel="stylesheet" href="libs/drmonty-leaflet-awesome-markers/css/leaflet.awesome-markers.css">
  <link rel="stylesheet" href="libs/Humanitarian-Font/assets/css/font-humanitarian.css">
  <link rel="stylesheet" href="libs/leaflet.markercluster/dist/MarkerCluster.css">
  <link rel="stylesheet" href="libs/leaflet.markercluster/dist/MarkerCluster.Default.css">
  <link rel="stylesheet" href="libs/mapbox.js/theme/style.css">

  <script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
